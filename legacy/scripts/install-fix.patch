diff --git a/install.sh b/install.sh
--- a/install.sh
+++ b/install.sh
@@ -103,7 +103,8 @@ get_asset_url() {
     local arch="$2"
     local api_url="https://api.github.com/repos/$REPO/releases/tags/$version"
 
-    log_info "Querying release metadata for $version..."
+    # Query metadata (log to stderr to not interfere with return value)
+    log_info "Querying release metadata for $version..." >&2
     local json
     json="$(gh_api "$api_url")"
 
@@ -223,20 +224,38 @@ download_and_install() {
             ;;
     esac
 
-    # Install CLI binary (expects executable named "MASH" somewhere in archive)
+    # Install CLI binary
+    # The tarball has structure: mash-installer-ARCH/mash-installer-ARCH
     local cli_binary
-    cli_binary="$(find . -type f -name 'MASH' -perm /111 -print -quit 2>/dev/null || true)"
+    
+    # Look for architecture-specific binary in subdirectory
+    if [[ -d "mash-installer-${arch}" ]]; then
+        cli_binary="$(find "mash-installer-${arch}" -type f -executable -print -quit 2>/dev/null || true)"
+    fi
+    
+    # Fallback: search for any executable named mash-installer*
     if [[ -z "$cli_binary" ]]; then
-        # fallback: allow "mash-installer" too
-        cli_binary="$(find . -type f -name 'mash-installer' -perm /111 -print -quit 2>/dev/null || true)"
+        cli_binary="$(find . -type f -name 'mash-installer*' -executable -print -quit 2>/dev/null || true)"
+    fi
+    
+    # Fallback: look for MASH or mash-installer anywhere
+    if [[ -z "$cli_binary" ]]; then
+        cli_binary="$(find . -type f \( -name 'MASH' -o -name 'mash-installer' \) -executable -print -quit 2>/dev/null || true)"
     fi
 
     if [[ -n "$cli_binary" ]]; then
         log_info "Installing CLI from $cli_binary..."
-        cp "$cli_binary" "$INSTALL_DIR/MASH"
-        chmod +x "$INSTALL_DIR/MASH"
-        log_success "CLI installed to $INSTALL_DIR/MASH"
+        cp "$cli_binary" "$INSTALL_DIR/mash-installer"
+        chmod +x "$INSTALL_DIR/mash-installer"
+        log_success "CLI installed to $INSTALL_DIR/mash-installer"
     else
         log_error "CLI binary not found in extracted files"
@@ -246,17 +265,25 @@ download_and_install() {
         exit 1
     fi
 
-    # Optional Qt GUI: looks for an executable named "MASH-qt"
+    # Optional Qt GUI
     local qt_binary
-    qt_binary="$(find . -type f -name 'MASH-qt' -perm /111 -print -quit 2>/dev/null || true)"
+    
+    # Look in qt subdirectory first
+    if [[ -d "mash-installer-qt" ]]; then
+        qt_binary="$(find "mash-installer-qt" -type f -executable -print -quit 2>/dev/null || true)"
+    fi
+    
+    # Fallback: search anywhere
+    if [[ -z "$qt_binary" ]]; then
+        qt_binary="$(find . -type f -name '*qt*' -executable -print -quit 2>/dev/null || true)"
+    fi
+    
     if [[ -n "$qt_binary" ]]; then
         log_info "Installing Qt GUI from $qt_binary..."
-        cp "$qt_binary" "$INSTALL_DIR/MASH-qt"
-        chmod +x "$INSTALL_DIR/MASH-qt"
-        log_success "Qt GUI installed to $INSTALL_DIR/MASH-qt"
+        cp "$qt_binary" "$INSTALL_DIR/mash-installer-qt"
+        chmod +x "$INSTALL_DIR/mash-installer-qt"
+        log_success "Qt GUI installed to $INSTALL_DIR/mash-installer-qt"
     else
         log_warning "Qt GUI not found in archive (optional component)"
     fi
